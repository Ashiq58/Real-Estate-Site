<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Reale state Deshbord", "Admin", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li class="active">@Html.ActionLink("Home", "Admin", "Home")</li>
                    <li class="dropdown">
                        <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                            References
                            <span class="caret"></span>
                        </a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("Country", "Index", "Country")</li>
                            <li>@Html.ActionLink("City", "Index", "City")</li>
                        </ul>
                    </li>

                    <li class="dropdown">
                        <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                            Users
                            <span class="caret"></span>
                        </a>
                        <ul class="dropdown-menu">

                            <li>@Html.ActionLink("Users", "Index", "Users")</li>                      
                            <li>@Html.ActionLink("UserImage", "Index", "UserImage")</li>
                            
                        </ul>
                    </li>
                    <li class="dropdown">
                        <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                            Flat
                            <span class="caret"></span>
                        </a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("Flat", "Index", "Flat")</li>
                            <li>@Html.ActionLink("FlatImage", "Index", "FlatImage")</li>
                        </ul>
                    </li>

                    <li class="dropdown">
                        <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                            Property
                            <span class="caret"></span>
                        </a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("Property", "Index", "Property")</li>
                            <li>@Html.ActionLink("Image", "Index", "PropertyImage")</li>    
                            
                                                      
                        </ul>
                    </li>
                    <li class="dropdown">
                        <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                           Booking
                            <span class="caret"></span>
                        </a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("Booking List", "Index", "BooKing")</li>                       

                        </ul>
                    </li>
                    <li class="dropdown">
                        <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                            Payment 
                            <span class="caret"></span>
                        </a>
                        <ul class="dropdown-menu">
                            @*<li>@Html.ActionLink("Buy/Sell", "Index", "Buy")</li>*@
                            <li>@Html.ActionLink("Rent", "Index", "Rent")</li>
                            <li>@Html.ActionLink("Rent Payment","Index","RentPayment")</li>  
                           <li>@Html.ActionLink("Payment", "Index", "Payment")</li>
                            <li>@Html.ActionLink("Discount", "Index", "Discount")</li>
                            <li>@Html.ActionLink("Voucher", "Index", "Voucher")</li>
                        </ul>
                    </li>

                    <li>@Html.ActionLink(" Message", "Index", "Queries")</li>

                </ul>
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>

            @Scripts.Render("~/bundles/jquery")
            <script src="~/Scripts/jquery.validate.min.js"></script>
        <script src="~/Scripts/jquery.validate.unobtrusive.js"></script>
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)

</body>
</html>
